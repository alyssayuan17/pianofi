{
  "family": "pianofi-amtworkers",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::678597489359:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::678597489359:role/pianofi-task-role",
  "containerDefinitions": [{
    "name": "amtworkers",
    "image": "678597489359.dkr.ecr.us-east-1.amazonaws.com/pianofi/amtworkers:latest",
    "essential": true,
    "environment": [
      {"name": "AWS_REGION", "value": "us-east-1"},
      {"name": "USE_LOCAL_STORAGE", "value": "false"}
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/pianofi-amtworkers",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs",
        "awslogs-create-group": "true"
      }
    }
  }]
}